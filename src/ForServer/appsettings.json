{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "app_data/app.logs",
      "Append": true,
      "FileSizeLimitBytes": 200000,
      "MaxRollingFiles": 5
    },
    "MemoryLogger": {
      "LogLevel": {
        "Default": "Trace"
      } 
    }
  },
  "AllowedHosts": "*"
}
