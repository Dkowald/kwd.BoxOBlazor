{
  "CircuitOptions": {
    "DetailedErrors": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "kwd.BoxOBlazor": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "app_data/app.logs",
      "Append": true,
      "FileSizeLimitBytes": 200000,
      "MaxRollingFiles": 5
    },
    "MemoryLogger": {
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "AllowedHosts": "*",

  "SiteConfig": {
    //Url to the WASM version
    "SpaUrl": "https://localhost:44353/"
  }
}
